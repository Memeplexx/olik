{
  "name": "olik-ng",
  "version": "0.0.7",
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "browser": "dist/index.bundle.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist coverage",
    "tsc": "tsc && node node_modules/gulp/bin/gulp.js",
    "build": "rollup -c && npm run tsc",
    "dev": "rollup -c --watch",
    "test": "jest --coverage --coverageReporters=text-lcov",
    "test:cov": "npm test --coverage --collectCoverageFrom=src/**/*.{ts}",
    "test:watch": "jest --watchAll --verbose false"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@angular/core": "~10.2.0",
    "olik": "0.0.42",
    "rxjs": "~6.5.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.10",
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.2",
    "del": "^5.1.0",
    "eslint": "^7.31.0",
    "gulp": "^4.0.2",
    "jest": "^26.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.23.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.3",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-jest": "^26.1.4",
    "typescript": "3.9.9"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
