{
  "name": "olik-react",
  "version": "0.0.2",
  "description": "",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "unpkg": "dist/umd/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "clean": "rimraf dist coverage",
    "tsc": "tsc && node node_modules/gulp/bin/gulp.js",
    "build": "rollup -c && npm run tsc",
    "dev": "rollup -c --watch",
    "test": "jest --coverage --coverageReporters=text-lcov",
    "test:cov": "npm test --coverage --collectCoverageFrom=src/**/*.{ts}",
    "test:watch": "jest --watchAll --verbose false"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "olik": "0.0.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.7",
    "@types/react": "^16.9.44",
    "cross-env": "^7.0.2",
    "del": "^5.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "gulp": "^4.0.2",
    "jest": "^26.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.23.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.3",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-jest": "^26.1.4",
    "typescript": "3.9.9"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
