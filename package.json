{
  "name": "olik",
  "version": "0.1.21",
  "description": "Declarative state-management. Free of inaccurate actions & convoluted reducers. All in-line.",
  "license": "MIT",
  "homepage": "https://memeplexx.github.io/Olik/",
  "bugs": "https://github.com/Memeplexx/Olik/issues",
  "keywords": [
    "state",
    "predictable",
    "immutable",
    "fluent",
    "react",
    "angular",
    "javascript",
    "typescript"
  ],
  "author": "Stephen Paul (https://github.com/memeplexx)",
  "main": "dist/cjs",
  "module": "dist/esm",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist coverage",
    "build": "rimraf dist && tsc && rollup -c rollup.config.js && rimraf out-tsc",
    "dev": "rollup -c --watch",
    "test": "cross-env NODE_ENV=test jest --coverage --coverageReporters=text-lcov | coveralls",
    "test:cov": "cross-env NODE_ENV=test npm test --coverage --collectCoverageFrom=src/**/*.{ts}",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll --verbose false"
  },
  "dependencies": {
  },
  "devDependencies": {
    "@types/immutable": "^3.8.7",
    "@types/jest": "^26.0.7",
    "@types/react": "^16.9.46",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.2",
    "del": "^5.1.0",
    "immer": "^7.0.7",
    "immutable": "^4.0.0-rc.12",
    "jest": "^26.1.0",
    "react": "^16.13.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.23.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.3",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "rxjs": "~6.5.4",
    "ts-jest": "^26.4.4",
    "typescript": "3.8"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
