{
  "name": "olik",
  "version": "1.0.104",
  "type": "module",
  "main": "./dist/olik.cjs",
  "module": "./dist/olik.js",
  "sideEffects": false,
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts"
      ],
      "devtools": [
        "dist/devtools.d.ts"
      ],
      "derive": [
        "dist/derive.d.ts"
      ],
      "async": [
        "dist/write-async.d.ts"
      ],
      "async-query": [
        "dist/async-query.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "import": "./dist/olik.js",
      "require": "./dist/olik.cjs"
    },
    "./devtools": {
      "import": "./dist/olik2.js",
      "require": "./dist/olik2.cjs"
    },
    "./derive": {
      "import": "./dist/olik3.js",
      "require": "./dist/olik3.cjs"
    },
    "./async": {
      "import": "./dist/olik4.js",
      "require": "./dist/olik4.cjs"
    },
    "./async-query": {
      "import": "./dist/olik5.js",
      "require": "./dist/olik5.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.44.0",
    "immer": "^7.0.7",
    "immutable": "^4.0.0-rc.12",
    "jest": "^29.4.3",
    "rxjs": "~6.5.4",
    "ts-jest": "^29.0.5",
    "typescript": "5.2.2",
    "vite": "^4.4.0",
    "vite-plugin-dts": "^1.7.3",
    "vitest": "^0.28.5"
  },
  "description": "A State Management library with a focus on Type-Safety, and inline state updates",
  "author": "Stephen Paul",
  "license": "MIT",
  "homepage": "https://github.com/Memeplexx/olik",
  "repository": {
    "type": "git",
    "url": "https://github.com/Memeplexx/olik.git"
  },
  "bugs": {
    "url": "https://github.com/Memeplexx/olik/issues"
  },
  "keywords": [
    "State Management",
    "Fluent API",
    "Boilerplate free",
    "Type Safe"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "build:analyze": "npx vite-bundle-visualizer --output ./stats.html"
  }
}